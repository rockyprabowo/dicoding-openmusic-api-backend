{
  "name": "@openmusic/api",
  "version": "1.0.0",
  "description": "Dicoding Fundamental Backend Class Submission - OpenMusic API",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=production node .",
    "dev": "nodemon -w src",
    "docs:build": "jsdoc -c jsdocs.conf.json",
    "docs:dev": "nodemon --exec 'jsdoc -c jsdocs.conf.json' --watch ./src",
    "lint": "eslint ./src",
    "test": "node tests/newman",
    "keys:generate": "node . --env-generate-keys",
    "db:create": "node . --create-db",
    "migrate": "node-pg-migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rockyprabowo/dicoding-openmusic-api-backend.git"
  },
  "keywords": [
    "dicoding",
    "backend-course"
  ],
  "author": "Rocky Prabowo <rocky@lazycats.id>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rockyprabowo/dicoding-openmusic-api-backend/issues"
  },
  "homepage": "https://github.com/rockyprabowo/dicoding-openmusic-api-backend#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/hapi__hapi": "^20.0.10",
    "@types/hapi__inert": "^5.2.3",
    "@types/hapi__jwt": "^2.0.1",
    "@types/newman": "^5.3.0",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^39.2.1",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jsdoc": "^3.6.10",
    "jsdoc-ts-utils": "^2.0.1",
    "newman": "^5.3.2",
    "nodemon": "^2.0.16"
  },
  "dependencies": {
    "@hapi/hapi": "^20.2.1",
    "@hapi/inert": "^6.0.5",
    "@hapi/jwt": "^2.1.1",
    "amqplib": "^0.8.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "joi": "^17.6.0",
    "module-alias": "^2.2.2",
    "nanoid": "^3.3.4",
    "node-pg-migrate": "^6.2.1",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4"
  },
  "_moduleAliases": {
    "@root": ".",
    "~": "./src",
    "~types": "./src/types",
    "@api": "./src/api",
    "@data": "./src/data",
    "@exceptions": "./src/exceptions",
    "@services": "./src/services",
    "@utils": "./src/utils",
    "@validators": "./src/validators"
  }
}
