{
  "plugins": [
    "jsdoc-ts-utils",
    "jsdoc-plugin-intersection"
  ],
  "source": {
    "include": [
      "./backend/src",
      "./common/src",
      "./consumer/src"
    ]
  },
  "opts": {
    "encoding": "utf8",
    "private": true,
    "destination": "./backend/public/devdocs/",
    "recurse": true,
    "readme": "./README.md"
  }
}
